#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    // Input
    int n;
    cin >> n;

    vector<int> arr(n);

    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }

    // Sort the list
    sort(arr.begin(), arr.end());

    // Find and print the middle element
    int middleIndex = n / 2;
    cout << arr[middleIndex] << endl;

    return 0;
}
